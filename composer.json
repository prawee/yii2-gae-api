{
    "name": "prawee/yii2-gae-api",
    "version": "1.0",
    "description": "Create RESTFul for deploy on Google App Engine with Yii2",
    "type": "project",
    "keywords": [
      "GAE", "Yii2", "RESTful", "API", "google app engine"
    ],
    "license": "MIT",
    "homepage": "https://github.com/prawee/yii2-gae-api.git",
    "authors": [
        {
            "name": "Prawee Wongsa",
            "email": "prawee@hotmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "require-dev": {
        "yiisoft/yii2-gii": "^2.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
